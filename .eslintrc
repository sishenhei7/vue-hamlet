{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "no-debugger": 0,
    "no-shadow": 0,
    "no-return-assign": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "max-len": [
      "error",
      {
        "ignoreStrings": true,
        "ignoreRegExpLiterals": true,
        "code": 120
      }
    ]
  }
}
